{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "paths": {
      "@spartan-ng/helm/button": [
        ".\\libs/ui/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        ".\\libs/ui/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/helm/card": [
        ".\\libs/ui/ui-card-helm/src/index.ts"
      ],
      "@spartan-ng/helm/avatar": [
        ".\\libs/ui/ui-avatar-helm/src/index.ts"
      ],
      "@spartan-ng/helm/alert": [
        ".\\libs/ui/ui-alert-helm/src/index.ts"
      ],
      "@spartan-ng/helm/dialog": [
        ".\\libs/ui/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/helm/label": [
        ".\\libs/ui/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/helm/select": [
        ".\\libs/ui/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/helm/switch": [
        ".\\libs/ui/ui-switch-helm/src/index.ts"
      ],
      "@spartan-ng/helm/tabs": [
        ".\\libs/ui/ui-tabs-helm/src/index.ts"
      ],
      "@spartan-ng/helm/badge": [
        ".\\libs/ui/ui-badge-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
